    "def staircase(n):\n",
    "    for i in range(n-1,-1,-1):\n",
    "        for j in range(i):\n",
    "            print(\" \",end = \"\")\n",
    "        for k in range(i,n):    \n",
    "            print(\"#\",end = \"\")\n",
    "        print(\"\")   \n",
    "staircase(10)"
